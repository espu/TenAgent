{
  "type": "extension",
  "name": "pcm_file_reader_python",
  "version": "0.11.53",
  "display_name": {
    "locales": {
      "en-US": {
        "content": "PCM File Reader Python Extension"
      },
      "zh-CN": {
        "content": "PCM 文件读取 Python 扩展"
      }
    }
  },
  "description": {
    "locales": {
      "en-US": {
        "content": "A Python extension that reads PCM audio files and sends audio frames at configurable intervals"
      },
      "zh-CN": {
        "content": "读取 PCM 音频文件并按可配置的间隔发送音频帧的 Python 扩展"
      }
    }
  },
  "tags": [
    "python",
    "audio"
  ],
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.11.53"
    }
  ],
  "api": {
    "property": {
      "properties": {
        "audio_file_path": {
          "type": "string"
        },
        "sample_rate": {
          "type": "int32"
        },
        "bytes_per_sample": {
          "type": "int32"
        },
        "number_of_channels": {
          "type": "int32"
        },
        "frame_interval_ms": {
          "type": "int32"
        }
      }
    },
    "audio_frame_out": [
      {
        "name": "pcm_frame"
      }
    ],
    "cmd_in": [
      {
        "name": "get_status"
      }
    ]
  }
}