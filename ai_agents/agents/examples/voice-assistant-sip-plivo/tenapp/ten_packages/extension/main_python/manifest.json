{
  "type": "extension",
  "name": "main_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.11"
    },
    {
      "type": "system",
      "name": "ten_ai_base",
      "version": "0.7"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "**.tent",
      "**.py",
      "README.md",
      "tests/**"
    ]
  },
  "api": {
    "property": {
      "properties": {
        "greeting": {
          "type": "string"
        },
        "plivo_auth_id": {
          "type": "string"
        },
        "plivo_auth_token": {
          "type": "string"
        },
        "plivo_from_number": {
          "type": "string"
        },
        "plivo_server_port": {
          "type": "int32"
        },
        "plivo_public_server_url": {
          "type": "string"
        },
        "plivo_use_https": {
          "type": "bool"
        },
        "plivo_use_wss": {
          "type": "bool"
        }
      }
    }
  }
}
