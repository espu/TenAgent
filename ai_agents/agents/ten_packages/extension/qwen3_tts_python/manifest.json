{
  "type": "extension",
  "name": "qwen3_tts_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.11"
    },
    {
      "type": "system",
      "name": "ten_ai_base",
      "version": "0.7"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "BUILD.gn",
      "**.tent",
      "**.py",
      "README.md",
      "requirements.txt"
    ]
  },
  "api": {
    "interface": [
      {
        "import_uri": "../../system/ten_ai_base/api/tts-interface.json"
      }
    ],
    "property": {
      "properties": {
        "params": {
          "type": "object",
          "properties": {
            "model": {
              "type": "string"
            },
            "language": {
              "type": "string"
            },
            "speaker": {
              "type": "string"
            },
            "instruct": {
              "type": "string"
            },
            "mode": {
              "type": "string"
            },
            "ref_audio_path": {
              "type": "string"
            },
            "ref_text": {
              "type": "string"
            },
            "voice_description": {
              "type": "string"
            },
            "device": {
              "type": "string"
            },
            "dtype": {
              "type": "string"
            },
            "attn_implementation": {
              "type": "string"
            }
          }
        },
        "dump": {
          "type": "bool"
        },
        "dump_path": {
          "type": "string"
        },
        "sample_rate": {
          "type": "int64"
        }
      }
    }
  }
}
